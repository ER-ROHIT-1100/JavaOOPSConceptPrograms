
package userdefinedobjects;

import java.util.Comparator;

public class Student implements Comparable<Student>{
	int marks;
	
	Student(int marks){
		this.marks=marks;
	}
	@Override
	public String toString() {
		return "Marks: "+marks;
	}
	 @Override
	public  int compareTO(Student x) {
		return this.marks-x.marks;
	}

}
